@model IEnumerable<MyMatch.Models.profile_viewers>


<script src="~/Scripts/jquery-1.8.2.js"></script>
<link href="~/Content/jquery.mobile.theme-1.1.2.css" rel="stylesheet" />
<link href="~/Content/jquery.mobile-1.1.2.css" rel="stylesheet" />

<p class="banner">
    @Html.ActionLink("Profile Viewers", "profile_viewer_Index", "Profile_Viewers")<a style="color:red">(@ViewBag.total)</a>
</p>

@foreach (var item in Model)
{
    
    <div data-role="header" data-theme="b">
        @Html.Action("profile_viewers_show", "Profile_Viewers", new { viewer = item.username })
    </div>
}

